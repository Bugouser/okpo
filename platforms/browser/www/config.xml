<?xml version='1.0' encoding='utf-8'?>
<widget id="com.okpowb.okpo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>ОКПО</name>
    <description>
        Поиск ОКПО по ИНН
    </description>
    <author email="okpowb@gmail.com">
        White Black
    </author>
    <content src="index.html" />
    <icon src="www/img/appicon.png" />
    <splash src="www/img/splash.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="show-splash-screen-spinner" value="false" />
    <preference name="auto-hide-splash-screen" value="false" />
    <preference name="FadeSplashScreen " value="false" />
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
</widget>
